                "dataSets": [
                  {
                    "timeSeriesFilter": {
                        "filter": "metric.type=\"custom.googleapis.com/spinnaker/platform/kubernetes/api/completion_latencies\"",
                        "perSeriesAligner": "ALIGN_DELTA",
                        "crossSeriesReducer": "REDUCE_MEAN",
                        "secondaryCrossSeriesReducer": "REDUCE_NONE",
                        "minAlignmentPeriod": "60s",
                        "groupByFields": [
                            "metric.label.method"
                        ],
                        "unitOverride": "ns"
                    }
                  }
                ],
                "constantLines": [
                  {}
                ],
                "options": {},
                "y1Axis": {},
                "xAxis": {}
              }
            }
          ]
